{% macro renderBook(controller, book) %}
    {% from 'Macro/Book.html.twig' import ranking %}

    <div class="col-book style-1">
        <div class="dz-shop-card style-1">
            <div class="dz-media">
                <img src="{{ controller.getUrlBookImage(book) }}" alt="book">
            </div>
            <div class="dz-content">
                <h5 class="title"><a href="BookDetail?code={{ book.id }}">{{ book.name }}</a></h5>
                {{ book.author }}
                {{ ranking(controller, book) }}
                <a href="BookDetail?code={{ book.id }}" class="btn btn-secondary box-btn btnhover2 mt-2">Saber más</a>
            </div>
        </div>
    </div>
{% endmacro %}

{% macro renderCategoryFilter(controller) %}
    {% for category in controller.getCategories() %}
        {% set checkID = 'categoryCheck_' ~ category.id %}
        <div class="form-check search-content">
            <input class="form-check-input" type="checkbox" value="" id="{{ checkID }}">
            <label class="form-check-label" for="{{ checkID }}">{{ category.name }}</label>
        </div>
    {% endfor %}
{% endmacro %}
